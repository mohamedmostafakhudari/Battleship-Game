{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"outDir": "dist",
		"sourceMap": true,
		"baseUrl": "src",
		"noEmit": false,
		"paths": {
			"@/*": ["*"]
		}
	}
}
